@model IEnumerable<LoginTestDemo.Models.tblSkill>

@{
    ViewBag.Title = "Dashboard_Employee";
}

<h2>Dashboard_Employee</h2>

<p>
    @Html.ActionLink("Add Skill", "User_Data_Entry")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Skill Name")
        </th>
        <th>
            @Html.DisplayName("Experience(In Months)")
        </th>
        <th>
            @Html.DisplayName("Previous Projects")
        </th>
        <th>
            @Html.DisplayName("Current Project")
        </th>
        <th>
            @Html.DisplayName("Domain")
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>

        @Html.DisplayFor(modelItem => item.skName)
    </td>
    
    <td>
        @Html.DisplayFor(modelItem => item.skExperience)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.skPrevProjects)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.skCurrentProject)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.skDomain)
    </td>

    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.skId }) |
        @Html.ActionLink("Details", "Details", new { id = item.skId }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.skId })
    </td>
</tr>
}

</table>
